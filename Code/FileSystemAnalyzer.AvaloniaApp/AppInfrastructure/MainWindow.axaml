<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:avaloniaApp="clr-namespace:FileSystemAnalyzer.AvaloniaApp"
        xmlns:appInfrastructure="clr-namespace:FileSystemAnalyzer.AvaloniaApp.AppInfrastructure"
        mc:Ignorable="d"
        x:Class="FileSystemAnalyzer.AvaloniaApp.AppInfrastructure.MainWindow"
        MinWidth="800"
        MinHeight="600"
        x:DataType="appInfrastructure:MainWindowViewModel"
        Title="File System Analyzer">
    <TransitioningContentControl Content="{CompiledBinding CurrentContent, Mode=OneWay}" />
</Window>